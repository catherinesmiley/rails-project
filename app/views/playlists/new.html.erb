<%= render partial: '/application/errors_messages', locals: {object: @playlist} %>

<h1>Create a new playlist</h1>

<%= form_for @playlist do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>
    <%= f.label :description %>
    <%= f.text_area :description %><p>

    <%= f.submit "Create playlist" %>
<% end %>
